.navbar
  .navbar-inner
    .left
      a.back.b-nav-button.link.b-back(href='#'): svg.b-back__icon: use(xlink:href='#icon_back')
    .center.sliding Карта
    .right
      a.link.b-nav-button(href='#', id='watch-me')
        svg.b-filter-ctrl__icon
          use(xlink:href='#icon_map-location')
      a.link.open-panel.b-nav-button(href='#')
        .b-nav-button__body
          .b-nav-button__item.b-nav-button__item_top
          .b-nav-button__item.b-nav-button__item_center
          .b-nav-button__item.b-nav-button__item_bottom
.pages
  .page.page-no-space(data-page='map')
    .page-content(id='map-map', class='active')
      .empty-page(data-text='Загрузка карты...')
    include ../ui/toolbar.pug

